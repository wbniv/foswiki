key.base = cased
key.number.separator = {}
key.format = 
{
  %s(bibkey)
  # {
      { 
	  { 
	    { %2.2#p(author) %2n(author) } #
	    %1n(author) 
	  } # 
	  %1n(editor) #
	  %999W(title) 
      }
      {
	%2d(year) #
      }
  }
  # 
  **key*
}
