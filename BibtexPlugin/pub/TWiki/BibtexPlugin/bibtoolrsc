sort.format = 
{
  %s(bibkey)
  # {
      { 
	  %n(author) # 
	  %n(editor) #
	  %n(publisher) #
      }
      {
	%2d(year) #
      }
      {
	%s(booktitle) #
	%s(journal) #
      }
      :%s($type)
      :%999W(title) 
  }
}
