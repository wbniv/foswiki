BEGIN {
    # See the 'view' script for a description of what this does
    if( $ENV{"SCRIPT_FILENAME"} &&
        $ENV{"SCRIPT_FILENAME"} =~ /^(.+)\/[^\/]+$/ ) {
        chdir $1;
    }
    unshift @INC, '.';
    require 'setlib.cfg';
}

use TWiki::UI;
use TWiki::UI::Manage;
TWiki::UI::run( \&TWiki::UI::Manage::manage, manage=>1 );
