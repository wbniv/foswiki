---+ ToPDFAddon
$Foswiki::cfg{Plugins}{ToPDFAddon}{DefaultEncoding}="iso-8859-1";
$Foswiki::cfg{Plugins}{ToPDFAddon}{GhostscriptPath}="/usr/bin/gs";
$Foswiki::cfg{Plugins}{ToPDFAddon}{FontsPath}="/usr/share/fonts/default/ghostscript/";
$Foswiki::cfg{Plugins}{ToPDFAddon}{PrintTemplate}="bla";
$Foswiki::cfg{Plugins}{ToPDFAddon}{CreateTOC}=0;
$Foswiki::cfg{Plugins}{ToPDFAddon}{BaseUrl}="foswiki.org";
$Foswiki::cfg{Plugins}{ToPDFAddon}{RenderImages}=1;
$Foswiki::cfg{Plugins}{ToPDFAddon}{RenderForms}=0;
$Foswiki::cfg{Plugins}{ToPDFAddon}{RenderLinks}=1;
$Foswiki::cfg{Plugins}{ToPDFAddon}{PageWidth}=755;
$Foswiki::cfg{Plugins}{ToPDFAddon}{MediaType}="A4";
$Foswiki::cfg{Plugins}{ToPDFAddon}{DateFormat}="d.m.y";

