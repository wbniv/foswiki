  local i=0
  while let "i<=$1"; do N[i]=$i; let i=i+1
  done
}
export s='[ 	]'
export nots='[^ 	]'
export KSCOLORBG='#FFFFFF'
export KSCOLORUNSEL='#e0e0c0'
export KSCOLORSEL='#ffddaa'
