export margin="&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;"
export KSNOEXPAND=false
export KSLEFTTOPICS='%TMPL:INCLUDE{"ks_lefttopics"}%'
export KSNOLEFTTOPICS='%TMPL:INCLUDE{"ks_nolefttopics"}%'
export KSNOWEBSBAR=
export KSWEBSRELATEDBAR=
